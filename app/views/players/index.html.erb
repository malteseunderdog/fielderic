<h1>Listing players</h1>

<table>
  <tr>
    <th>Nickname</th>
    <th>Name</th>
    <th>Email</th>
    <th>Mobile</th>
    <th>Location</th>
  </tr>

<% @player.each do |player| %>
  <tr>
    <td><%=h player.nickname %></td>
    <td><%=h player.name %></td>
    <td><%=h player.email %></td>
    <td><%=h player.mobile %></td>
    <td><%=h player.location %></td>    
    <td><%= link_to 'Show', player %></td>
    <td><%= link_to 'Edit', edit_player_path(player) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Register', new_player_path %>