.navbar .dropdown-menu {
	margin-top: 5px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}
a{
    color: #F1962E;
}
a:hover{
    color: #d78629;
}
h6{
    font-weight: 400;font-style: italic;text-transform: none;color: #888;font-size: 12px;
}
.nav-pills .active &gt; a, .nav-pills .active &gt; a:hover{
    background-color: #F1962E;
}
#mainmenu{
    top: 40px;
}
.navbar-inner{
    background: none repeat scroll 0 0 #222;
}
.navbar .nav .active &gt; .dropdown-toggle, .navbar .nav .open.active &gt; .dropdown-toggle{
    background-color: rgba(0, 0, 0, 0.5);
}
#mainmenu li{
    font-size: 14px;text-transform: uppercase;font-weight: bold;
}
#mainmenu ul.dropdown-menu li{
    font-size: 13px;text-transform: none;font-weight: normal;
}
#mainbg-wrapper, #mainbg-wrapper2{
    overflow: hidden;width: 100%;
}
#mainbg{
    background-attachment: scroll;background-clip: border-box;background: #333333 url(/img/x.gif) no-repeat center bottom;background-origin: padding-box;background-size: cover;height: 460px;
}
.cmf-skinned-text{
    line-height: 25px;font-weight: 300;
}
.inpt{
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;border: 1px solid #CCCCCC;border-radius: 3px 3px 3px 3px;position: relative;padding: 4px 4px 4px 75px;margin: 12px 0;
}
#content .container{
    padding: 20px 15px 70px;background: none repeat scroll 0 0 rgba(255, 255, 255, 0.65);box-shadow: 6px 0 14px-4px rgba(90,90,90,0.2) , -6px 0 14px-4px rgba(90,90,90,0.2);
}
#heading .container{
    padding: 8px 15px;background: none repeat scroll 0 0 rgba(255, 255, 255, 0.65);box-shadow: 6px 0 14px-4px rgba(90,90,90,0.2) , -6px 0 14px-4px rgba(90,90,90,0.2);
}
#content div.span3{
    min-height: 400px;
}
div.home h1, div.dashboard h1, div.listing h1, div.city h1{
    font-size: 21px;line-height: 27px;margin: 0 0 9px;
}
div.home h3{
    font-size: 26px;line-height: 29px;font-weight: 200;margin: 20px 0 8px 0;
}
div.home h4{
    font-size: 13px;line-height: 18px;margin: 0 0 9px;font-weight: normal;
}
ul#login{
    width: 390px;max-width: 390px;
}
ul#login input.btn-primary{
    color: #fff;margin-top: 0;
}
ul#login li{
    padding: 0;
}
ul#login a{
    display: inline;padding: 0;margin: 0;text-decoration: underline;
}
ul#login li div{
    padding: 0 20px;margin: 12px 0;
}
ul#login input#sbmt{
    width: 75px;
}
form#lm{
    padding: 12px 20px;background: #f5f5f5;margin: 12px 0;border-top: 1px solid #B4B6AC;border-bottom: 1px solid #B4B6AC;
}
form#lm label{
    width: 75px;
}
[class^="icon-"]{
    margin-top: 1px;position: relative;
}
#breadcrumb{
    margin-bottom: 14px;
}
#loader{
    background:#111;background:rgba(0,0,0,.8);color:#eee;font-size:1.2em;padding:1em 2em;text-shadow:0 1px 0 #000;-moz-border-radius:.6em;-ms-border-radius:.6em;-o-border-radius:.6em;-webkit-border-radius:.6em;border-radius:.6em;
}
#loader{
    left:50%;position:absolute;top:50%;z-index:90
}
.fbconnect{
	width:100px;		
}	
